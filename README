# 8

import random

def guessing_game():

    secret_number = random.randint(0, 10)
    
    tries = 0
    
    while True:
        guess = int(input("Player: "))
        tries += 1

        if guess == secret_number:
            print("That's right! Number of tries:", tries)
            break
        elif guess < secret_number:
            print("Try a greater number.")
        else:
            print("Try a smaller number.")
            
print("Welcome to the Guessing Game!")
guessing_game()
# 9

import random

def guessing_game(player):

    secret_number = random.randint(0, 10)
    
    tries = 0
    
    while True:
        guess = int(input(f"Player {player}: "))

        tries += 1

        if guess == secret_number:
            print(f"That's right! Number of tries: {tries}")
            return tries
        elif guess < secret_number:
            print("Try a greater number.")
        else:
            print("Try a smaller number.")
            
print("Welcome to the Guessing Game!")

player1_tries = guessing_game(1)

player2_tries = guessing_game(2)

if player1_tries < player2_tries:
    print("Winner is Player 1")
elif player1_tries > player2_tries:
    print("Winner is Player 2")
else:
    print("It's a tie!")
