# 6

p = int(input("Enter the first number: "))
k = int(input("Enter the second number: "))

if p % 2 == 0 and k % 2 == 0:
    print("Both numbers are even.")

elif p % 2 == 0 or k % 2 == 0:
    print("One of the numbers is even.")

else:
    print("Both numbers are odd.")

